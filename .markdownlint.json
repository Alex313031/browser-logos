{
    "header-style": {
        "style": "setext_with_atx"
    },
    "line_length": false,
    "no-duplicate-header": {
        "allow_different_nesting": true
    },
    "no-inline-html": {
        "allowed_elements": [
            "a",
            "br",
            "code",
            "img",
            "table",
            "tbody",
            "td",
            "th",
            "thead",
            "tr"
        ]
    }
}
